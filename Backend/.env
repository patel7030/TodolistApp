# Either provide a single connection string:
MYSQL_URL=mysql://<user>:<password>@<host>:<port>/<database>

# OR provide parts (only if you don't use MYSQL_URL)
MYSQL_HOST=mysql-82ca.railway.internal
MYSQL_PORT=3306
MYSQL_USER=root
MYSQL_PASSWORD=your_password_here
MYSQL_DATABASE=railway

# Port (Railway will override with its assigned PORT env var at runtime)
PORT=5000
